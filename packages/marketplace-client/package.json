{
  "name": "@mcp-marketplace/client-sdk",
  "version": "1.0.1",
  "description": "TypeScript SDK for accessing the MCP App Marketplace API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "marketplace",
    "sdk",
    "typescript",
    "api-client"
  ],
  "author": "Giuseppe Matteo Gangemi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ggange/mcp-marketplace.git",
    "directory": "packages/marketplace-client"
  },
  "homepage": "https://github.com/ggange/mcp-marketplace#readme",
  "bugs": {
    "url": "https://github.com/ggange/mcp-marketplace/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@mcp-marketplace/types": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "typescript": "^5.3.3"
  }
}
